# Frontend Environment Configuration
# Copy to .env and adjust for your environment

# Port for the React dev server
PORT=3000

# Backend API base URL (Django)
# For production, set this to your public API base, e.g., https://api.your-domain.com/api/v1
REACT_APP_API_URL=http://localhost:8000/api/v1

# Avoid auto-opening browser on start (useful in containers)
BROWSER=none
